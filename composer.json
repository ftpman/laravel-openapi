{
  "name": "vyuldashev/laravel-openapi",
  "description": "Generate OpenAPI Specification for Laravel Applications",
  "keywords": [
    "laravel",
    "openapi",
    "api",
    "documentation",
    "docs",
    "rest",
    "swagger"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladimir Yuldashev",
      "email": "misterio92@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "doctrine/annotations": "^1.6",
    "doctrine/dbal": "^2.9",
    "goldspecdigital/oooas": "^2.1",
    "laravel/lumen-framework": "5.8.*",
    "phpdocumentor/reflection-docblock": "^4.3",
    "roave/better-reflection": "^3.5"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.8",
    "orchestra/testbench": "^3.8",
    "phpunit/phpunit": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "Vyuldashev\\LaravelOpenApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vyuldashev\\LaravelOpenApi\\Tests\\": "tests/",
      "Examples\\Petstore\\": "examples/petstore/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vyuldashev\\LaravelOpenApi\\OpenApiServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
